.muscilist{
  height:100rpx;
  padding:15rpx 0;
  border-bottom: 1px dotted #ccc;
}
.maintitle{
  height: 60rpx;
  line-height: 60rpx;
  border-left: 4px solid #444;
  padding-left: 10rpx;
  border-bottom: 1px solid #ccc;
}
.listimg{
  width: 100rpx;
  height: 100rpx;
  vertical-align: middle;
}
.musicbox{
  width:30%;
  margin-left: 2.5%;
  margin-top: 20rpx;
  float: left;
}
.musicbox image{
  width: 100%;
}
.desc{
  font-size: 24rpx;
  height: 40rpx;
  line-height: 30rpx;
  display: block;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.title{
  display: inline-block;
  width:480rpx;
  vertical-align:top;
  margin-left:20rpx;
}
.uptitle{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 34rpx;
  margin-top:5rpx;
}
.downtitle{
  display: inline-block;
  margin-right:10rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  margin-top: 10rpx;
  font-size: 24rpx;
}
.slide-image{
  width: 100%;
}